          <AnchorPane fx:id="innerScrollPane" minHeight="0.0" minWidth="0.0" prefHeight="512.0" prefWidth="185.0" />
        </content>
      </ScrollPane>
      <Pane layoutX="226.0" layoutY="14.0" prefHeight="180.0" prefWidth="160.0" style="-fx-background-color: #e8e8e8;">
