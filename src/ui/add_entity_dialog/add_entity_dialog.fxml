            <Label fx:id="entityDescription" alignment="TOP_LEFT" layoutX="13.0" layoutY="119.0" prefHeight="60.0" prefWidth="135.0" wrapText="true">
               <font>
                  <Font size="10.0" />
               </font>
               <padding>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
               </padding>
            </Label>
