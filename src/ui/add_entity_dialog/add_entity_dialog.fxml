<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.add_entity_dialog.AddEntityDialogController">
   <children>
      <ScrollPane hbarPolicy="NEVER" layoutX="14.0" layoutY="14.0" prefHeight="223.0" prefWidth="200.0" vbarPolicy="ALWAYS">
